package com.knorex.model;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ParkingLot parkingLot = new ParkingLot(2, 2);

        // Park vehicles
        parkingLot.parkVehicle("ABC123", "Red", VehicleType.CAR);
        parkingLot.parkVehicle("XYZ456", "Blue", VehicleType.CAR);

        // Try to park another vehicle (will print an error message)
        parkingLot.parkVehicle("DEF789", "Green", VehicleType.CAR);

        // Remove a vehicle
        parkingLot.removeVehicle("ABC123", 2);

        // Check availability on floor 1 for cars
        boolean availability = parkingLot.checkAvailability(1, VehicleType.CAR);
        System.out.println("Availability on Floor 1 for Cars: " + availability);

	}

}
